<map id="map" longitude="{{map.lng}}" latitude="{{map.lat}}" scale="14" markers="{{markers}}" bindmarkertap="markertap"  bindregionchange="regionchange" show-location style="width: 100%; height: 300px;"></map>
<import src="../../../wxParse/wxParse.wxml"/>
<view class="map_text">
<view style="font-size: 30rpx;color: #ff5778;font-weight: bolder;" bindtap="bindgothere">
<text style="vertical-align:middle;">点击到这里去</text>
  <image src="../../../images/map1.png" style="width:30rpx;height:30rpx;vertical-align: middle;"></image>
</view>
<view class="map_address">
  <text>公司地址:{{address}}</text>
</view>
<view class="shoper_name">
  <text>公司名称:{{name}}</text>
</view>
<view class="contact_me">
  <text>公司电话:{{tel}}</text>
</view>
<view class="shoper_intor">
  <template is="wxParse" data="{{wxParseData:shoperIn.nodes}}">
     <text>公司简介: {{intro}}</text>
  </template>
</view>
</view>
<view class="tuoke" style="font-size:30rpx;text-align:center;padding:10rpx 0;" bindtap="go_tuoke">进入拓客 ></view>
<view>
  <view style="font-size: 25rpx;text-align: center;padding-top:50rpx;">{{yname.errMsg.banquan}}提供技术支持</view>
<view style="font-size: 25rpx;text-align: center;">{{yname.errMsg.url}}</view>
<view style="font-size: 25rpx;text-align: center;padding-bottom:50rpx;">联系电话:{{yname.errMsg.phone}}</view>
</view>